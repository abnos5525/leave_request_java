    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0k5k33e" name="Yes" sourceRef="Gateway_0wrzmwg" targetRef="Event_1uc840m">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == true}</bpmn:conditionExpression>
