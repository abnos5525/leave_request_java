      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">leaveType != "Hourly" || amount &gt;= 2</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:userTask id="Activity_08zj4x4" name="Manager Approval" camunda:formKey="manager_submition_form">
